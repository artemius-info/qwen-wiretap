{
  "name": "cq-wiretap",
  "version": "1.0.9",
  "description": "HTTP/HTTPS proxy for intercepting and visualizing Claude and Qwen API traffic",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "cq-wiretap": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup && pnpm run build:bundle-ui",
    "build:bundle-ui": "if not exist ./dist\\ui mkdir ./dist\\ui && xcopy /s /e /y /q ..\\ui\\dist\\*.* .\\dist\\ui\\ 2>nul || echo UI not built yet",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "claude",
    "qwen",
    "claude-code",
    "alibaba-cloud",
    "proxy",
    "wiretap",
    "debugging",
    "api",
    "anthropic",
    "openai",
    "cli",
    "inspector"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wierdbytes/cq-wiretap.git"
  },
  "license": "MIT",
  "dependencies": {
    "mockttp": "^3.10.0",
    "ws": "^8.16.0",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "open": "^10.1.0"
  },
  "devDependencies": {
    "@cq-wiretap/ui": "workspace:*",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=20"
  }
}
